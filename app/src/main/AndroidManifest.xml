<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.fitzeng.zzchat"
    android:installLocation="preferExternal">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".aty.AtyWelcome">
        </activity>
        <activity android:name=".aty.AtyGuide" />
        <activity android:name=".aty.AtyLoginOrRegister" />
        <activity android:name=".aty.AtyMain" />
        <activity android:name=".aty.AtyDressUp" />
        <activity android:name=".aty.AtyProfile" />
        <activity android:name=".aty.AtySetting" />
        <activity android:name=".aty.AtyChatRoom" />
        <activity android:name=".aty.AtyRegist"/>
        <activity android:name=".aty.AtyMainInterface"/>
        <activity android:name=".aty.AtyLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aty.ChooseDiary"/>
        <activity android:name=".aty.MyEditor"/>
    </application>

</manifest>